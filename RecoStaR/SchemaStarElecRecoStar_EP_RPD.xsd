			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RPD_RaccordementModulaire_RecoPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="RecoStaR:RPD_RaccordementModulaire_Reco"/>
		</xs:sequence>
