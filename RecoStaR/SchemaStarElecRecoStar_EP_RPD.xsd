					<xs:element name="noeudParent" type="RecoStaR:RPD_RaccordementModulaire_RecoPropertyType"/>
					<xs:element name="Coupure" type="xs:boolean"/>
					<xs:element name="NumeroPlage" type="xs:integer"/>
					<xs:element name="Protection" type="xs:boolean"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
