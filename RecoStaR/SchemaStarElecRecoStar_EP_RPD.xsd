	</xs:complexType>
	<xs:complexType name="RaccordementModulairePropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="RecoStaR:RaccordementModulaire"/>
		</xs:sequence>
		<xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
		<xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
	</xs:complexType>
	<xs:element name="RemonteeAeroSouterraine" type="RecoStaR:RemonteeAeroSouterraineType" substitutionGroup="RecoStaR:NoeudReseau" abstract="true">
		<xs:annotation>
			<xs:documentation>-- Définition --
Interconnexion entre un tronçon souterrain et un ou plusieurs tronçons aériens.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="RemonteeAeroSouterraineType" abstract="true">
		<xs:complexContent>
			<xs:extension base="RecoStaR:NoeudReseauType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RemonteeAeroSouterrainePropertyType">
		<xs:sequence minOccurs="0">
