					<xs:element name="PrecisionXY" type="RecoStaR:ClassePrecisionReseauValueType"/>
					<xs:element name="PrecisionZ" type="RecoStaR:ClassePrecisionReseauValueType"/>
					<xs:element name="ProfondeurMinNonReg" type="gml:MeasureType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
