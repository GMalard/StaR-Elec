							<xs:appinfo>
								<gmlexr:targetCodeList>CategoriesPosteValue</gmlexr:targetCodeList>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="Statut" type="RecoStaR:ConditionOfFacilityValueReco Type"/>
					<xs:element name="TypePoste" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo>
								<gmlexr:targetCodeList>TypePosteValue</gmlexr:targetCodeList>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RPD_PosteElectrique_RecoPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="RecoStaR:RPD_PosteElectrique_Reco"/>
		</xs:sequence>
