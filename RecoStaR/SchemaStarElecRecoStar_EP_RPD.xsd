					<xs:element name="DiametreDuFourreau" type="gml:MeasureType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>-- Définition --
Diamètre exprimé en mm</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Geometrie" type="gml:CurvePropertyType"/>
					<xs:element name="NombreOuvrages" type="xs:integer" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PrecisionXY" type="RecoStaR:ClassePrecisionReseauValueType"/>
					<xs:element name="PrecisionZ" type="RecoStaR:ClassePrecisionReseauValueType"/>
					<xs:element name="ProfondeurMinNonReg" type="gml:MeasureType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
