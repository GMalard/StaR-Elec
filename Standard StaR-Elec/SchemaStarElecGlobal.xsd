			<xs:extension base="star:NoeudReseauType">
				<xs:sequence>
					<xs:element name="NombrePlages" type="xs:integer" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
