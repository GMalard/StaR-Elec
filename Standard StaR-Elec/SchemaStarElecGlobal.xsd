								<gmlexr:targetCodeList>TemporaliteValue</gmlexr:targetCodeList>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="TensionUtilisation" type="gml:MeasureType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>-- Définition --
Tension d'utilisation (V)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Type" type="gml:ReferenceType" minOccurs="0" maxOccurs="1">
